{
  "name": "astx",
  "version": "0.0.0-development",
  "description": "super powerful structural search and replace for JavaScript and TypeScript to automate your refactoring",
  "astx": {
    "parser": "babel",
    "workers": 1,
    "parserOptions": {
      "preserveFormat": "generatorHack"
    },
    "preferSimpleReplacement": true
  },
  "sideEffects": false,
  "scripts": {
    "cli": "babel-node --extensions .ts src/cli/index.ts",
    "tsc:watch": "npm run tsc -- --watch",
    "tc": "toolchain",
    "toolchain": "toolchain",
    "test": "toolchain test",
    "prepublishOnly": "echo This package is meant to be published by semantic-release from the dist build directory. && exit 1"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codemodsquad/astx.git"
  },
  "keywords": [
    "ast",
    "automated",
    "codemod",
    "find",
    "javascript",
    "match",
    "matching",
    "pattern",
    "refactor",
    "refactoring",
    "replace",
    "rewrite",
    "rewriting",
    "search",
    "semantic",
    "structural",
    "syntax",
    "transform",
    "transformation",
    "typescript"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemodsquad/astx/issues"
  },
  "homepage": "https://github.com/codemodsquad/astx#readme",
  "devDependencies": {
    "@babel/node": "^7.13.13",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@jcoreio/eslint-plugin-implicit-dependencies": "^1.0.2",
    "@jcoreio/toolchain": "^3.9.3",
    "@jcoreio/toolchain-circle": "^3.9.3",
    "@jcoreio/toolchain-esnext": "^3.9.3",
    "@jcoreio/toolchain-mocha": "^3.9.3",
    "@jcoreio/toolchain-semantic-release": "^3.9.3",
    "@jcoreio/toolchain-typescript": "^3.9.3",
    "@types/babel__code-frame": "^7.0.3",
    "@types/babel__generator": "^7.6.5",
    "@types/babel__parser": "^7.1.1",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/diff": "^5.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.165",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.26",
    "@types/prettier": "^2.2.2",
    "@types/resolve": "^1.17.1",
    "@types/shallowequal": "^1.1.1",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@typescript-eslint/typescript-estree": "^4.14.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "copy": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "mocha": "^10.2.0",
    "require-glob": "^3.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "@babel/code-frame": "^7.22.13",
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.23.2",
    "@babel/types": "^7.23.0",
    "ansi-escapes": "^4.3.2",
    "ast-types": "^0.14.2",
    "babel-parse-wild-code": "^2.1.5",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.1",
    "debug": "^4.3.1",
    "dedent-js": "^1.0.1",
    "diff": "^5.0.0",
    "fs-extra": "^9.0.1",
    "gitignore-fs": "^2.2.0",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "minimatch": "^5.1.0",
    "p-event": "^4.0.0",
    "prettier": "^2.7.1",
    "recast": "^0.23.4",
    "resolve": "^1.19.0",
    "shallowequal": "^1.1.0",
    "tiny-typed-emitter": "^2.1.0",
    "ts-node": "^10.9.1",
    "yargs": "^17.6.2",
    "zod": "^3.22.4"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "astx": "./dist/cli/index.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.js",
      "types": "./dist/node/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.11.0"
}
